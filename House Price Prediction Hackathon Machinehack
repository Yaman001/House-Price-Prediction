   "source": [
    "# distance <1 : proximity,Dsitance<2 : vicinity,far\n",
    "def dist(x):\n",
    "    if (x<1):\n",
    "        return('proximity')\n",
    "    elif (x<2):\n",
    "        return('Vicinity')\n",
    "    else:\n",
    "        return('far')"
   ]
  },
