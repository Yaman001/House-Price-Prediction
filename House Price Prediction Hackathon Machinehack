    "north=['Delhi']\n",
    "east=['Kolkata']\n",
    "west=['Mumbai','Pune','Ahemdabad']\n",
    "south=['Chennai', 'Hyderabad','Banglore']\n",
    "def city(x):\n",
    "    if x in north:\n",
    "        return 'north'\n",
    "    elif x in east:\n",
    "        return 'east'\n",
    "    elif x in west:\n",
    "        return 'west'\n",
    "    else:\n",
    "        return 'south'"
   ]
